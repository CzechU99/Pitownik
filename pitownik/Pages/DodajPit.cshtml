    function generowaniePDF(){

        const kartki = document.querySelectorAll(".kartka");

        for (let i = 0; i < kartki.length; i++) {
            kartki[i].style.marginTop = "0";
        }

        window.scrollTo({
            top: 0,
            behavior: "instant"
        });
        
		html2pdf().set(options).from(div).save();

        window.setTimeout(() => {
        for (let i = 0; i < kartki.length; i++) {
            kartki[i].style.marginTop = "30px";
        };
        }, 2000);
    }

    btn.addEventListener('click', generowaniePDF);

    


</script>

